@page
@model AirplaneProject.Pages.CreateModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<p>Заполните данные:</p>

<form method="post">
    Логин:
    <input asp-for="CustomerUser!.Login" />
    Фамилия Имя Отчество:
    <input asp-for="CustomerUser!.Name" />
    Пароль:
    <input asp-for="CustomerUser!.Password" />
    Номер телефона:
    <input asp-for="CustomerUser!.PhoneNumber" />
    <input type="submit" value="Подтвердить"/>
</form>
